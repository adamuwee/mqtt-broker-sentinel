{
    "Name": "default",
    "mqtt_broker": {
        "connection": {
            "host_addr": "debian-openhab",
            "host_port": 1883
        },
        "process": {
            "name": "Notepad.exe",
            "service_wd_period_seconds": 10
        }
    },
    "topic_watchdog": {
        "all": {
            "max_time_seconds": 120
        },
        "amiweather/8/temperature": {
            "max_time_seconds": 60
        }
    },
    "publish": {
        "base_topic": "sc_mqtt_broker/",
        "process_status": "process_status",
        "topic_list": "topic_list",
        "missing_topics": "missing_topics"
    }
}